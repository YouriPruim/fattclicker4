<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/landing">

    <fragment
        android:id="@+id/landing"
        android:name="com.example.fattclicker3.landing"
        android:label="fragment_landing"
        tools:layout="@layout/fragment_landing" >
        <action
            android:id="@+id/action_landing_to_shop"
            app:destination="@id/shop" />
        <action
            android:id="@+id/action_landing_to_shop2"
            app:destination="@id/shop" />
    </fragment>
    <fragment
        android:id="@+id/shop"
        android:name="com.example.fattclicker3.shop"
        android:label="fragment_shop"
        tools:layout="@layout/fragment_shop" >
        <action
            android:id="@+id/action_shop_self"
            app:destination="@id/shop" />
    </fragment>
    <fragment
        android:id="@+id/instellingen"
        android:name="com.example.fattclicker3.instellingen"
        android:label="fragment_instellingen"
        tools:layout="@layout/fragment_instellingen" />
</navigation>